{
    "name": "bnomei/kirby3-autoid",
    "type": "plugin",
    "version": "1.2.6",
    "description": "Automatic unique ID for Pages, StructureObjects and Files including performant helpers to retrieve them. Bonus: Cache for collections and Tiny-URL.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Meilick",
            "email": "b@bnomei.com"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "autoid",
        "collection",
        "cache",
        "reference",
        "persistent",
        "structure-id",
        "tiny-url"
    ],
    "suggest": {
    },
    "autoload": {
        "files": [
            "config.php"
        ],
        "psr-4": {
            "Bnomei\\": "classes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1.0",
        "getkirby/cms": "^3.0.0-RC-2.0"
    },
    "conflict": {
        "getkirby/kirby": "<3.0"
    },
    "scripts": {
        "zip": [
            "touch kirby3-autoid.zip",
            "rm kirby3-autoid.zip",
            "composer install --no-dev",
            "composer remove getkirby/cms",
            "composer dumpautoload -o",
            "zip -r kirby3-autoid.zip . -x *.git*",
            "composer require getkirby/cms:'^3.0.0-RC-2.0'"
        ]
    }
}
